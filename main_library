//@version=5
strategy("AutoDollarVol ATR ‚Äî Fully Auto & FIXED (No Errors)", overlay=true,
     default_qty_type=strategy.percent_of_equity,
     default_qty_value=100,
     commission_type=strategy.commission.percent,
     commission_value=0.075,
     pyramiding=1,
     calc_on_every_tick=false,
     calc_on_order_fills=true)

// ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ INPUTS ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
ema_len            = input.int(50,   "EMA length (trend filter)", minval=8)
use_trend          = input.bool(true, "Use EMA trend filter")
atr_len            = input.int(14,   "ATR length", minval=2)
atr_mult           = input.float(1.5, "Initial stop √ó ATR", step=0.1)
reward_ratio       = input.float(2.0, "Reward : Risk ratio", step=0.1)
use_trailing       = input.bool(true, "Enable trailing stop")
trailing_after_atr = input.float(1.0, "Start trailing after X ATR profit", step=0.1)
trailing_atr_mult  = input.float(1.0, "Trailing distance √ó ATR", step=0.1)

risk_percent       = input.float(1.0, "Risk per trade (% of equity)", minval=0.05, step=0.05)
min_dvol_mult      = input.float(1.5, "Min dollar-volume vs average", step=0.1)
dvol_mult          = input.float(1.2, "Volume spike multiplier", step=0.1)
rsi_oversold       = input.int(30, "RSI oversold (longs)")
rsi_overbought     = input.int(70, "RSI overbought (shorts)")

allow_long         = input.bool(true,  "Allow Longs")
allow_short        = input.bool(true,  "Allow Shorts")
cooldown_bars      = input.int(1, "Cooldown bars between trades", minval=0)
max_trades_per_day = input.int(10, "Max trades/day (0=unlimited)", minval=0)
daily_loss_cap_pct = input.float(10.0, "Daily loss limit % (0=off)", step=0.1)
start_hour         = input.int(8,  "Session start (NY time)", minval=0, maxval=23)
end_hour           = input.int(23, "Session end (NY time)", minval=0, maxval=23)

// ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ INDICATORS ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
ema        = ta.ema(close, ema_len)
atr        = ta.atr(atr_len)
rsi        = ta.rsi(close, 14)

vol_spike  = volume > ta.sma(volume, 20) * vol_mult
dvol       = volume * close
dvol_ok    = dvol > ta.sma(dvol, 20) * min_dvol_mult

// Candle patterns (fixed & reliable)
bull_engulfing = close > open and close[1] < open[1] and close > open[1] and open <= close[1]
bear_engulfing = close < open and close[1] > open[1] and close < open[1] and open >= close[1]
hammer     = (math.min(close, open) - low) >= 2 * math.abs(close - open) and (high - math.max(close, open)) <= math.abs(close - open)
inv_hammer = (high - math.max(close, open)) >= 2 * math.abs(close - open) and (math.min(close, open) - low) <= math.abs(close - open)

bull_pattern = bull_engulfing or hammer
bear_pattern = bear_engulfing or inv_hammer

// NY session
in_session = hour(time("America/New_York")) >= start_hour and hour(time("America/New_York")) <= end_hour

// Daily counters
var int   trades_today      = 0
var float day_start_equity  = na
if ta.change(time("D"))
    trades_today     := 0
    day_start_equity := strategy.equity

daily_loss_ok = daily_loss_cap_pct <= 0 or (day_start_equity - strategy.equity) / day_start_equity * 100 < daily_loss_cap_pct
trades_limit_ok = max_trades_per_day == 0 or trades_today < max_trades_per_day

// Cooldown
var int last_entry_bar = na
can_enter = na(last_entry_bar) or bar_index - last_entry_bar > cooldown_bars

// ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ CONDITIONS ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
long_cond  = bull_pattern and vol_spike and dvol_ok and rsi > rsi_oversold and rsi < 80 and
             allow_long and in_session and daily_loss_ok and trades_limit_ok and can_enter and
             (not use_trend or close > ema) and strategy.position_size == 0

short_cond = bear_pattern and vol_spike and dvol_ok and rsi < rsi_overbought and rsi > 20 and
             allow_short and in_session and daily_loss_ok and trades_limit_ok and can_enter and
             (not use_trend or close < ema) and strategy.position_size == 0

// ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ POSITION SIZING ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
risk_qty(stop_price) =>
    risk_money = strategy.equity * risk_percent / 100
    dist = math.abs(close - stop_price)
    dist > syminfo.mintick ? risk_money / dist : 0

// ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ ENTRIES & EXITS ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
if long_cond
    sl = low - atr * atr_mult
    tp = close + (close - sl) * reward_ratio
    qty = risk_qty(sl)
    if qty > 0
        strategy.entry("Long", strategy.long, qty=qty)
        strategy.exit("Long Exit", "Long", stop=sl, limit=tp)
        trades_today += 1
        last_entry_bar := bar_index

if short_cond
    sl = high + atr * atr_mult
    tp = close - (sl - close) * reward_ratio
    qty = risk_qty(sl)
    if qty > 0
... (21 lines left)
