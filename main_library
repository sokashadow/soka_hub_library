                    hrp.Velocity = currentVel:Lerp(desiredVel, 0.2)
                end
            end
        end)
        table.insert(connections, speedConnection)
    end
end)

SpeedSection:NewSlider("Speed Value", "Adjust speed value", 500, 1, function(value)
    TargetSpeed = value
end, 1)

